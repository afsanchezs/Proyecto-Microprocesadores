void setup() {
  pinMode(A0,INPUT);
  pinMode(11,OUTPUT);
 Serial.begin(9600);

}


void loop() {
  analogWrite(11,0);
  Serial.println(analogRead(A0));
  delay(1000);
  analogWrite(11,100);
  Serial.println(analogRead(A0));
  delay(1000);
  analogWrite(11,255);
  Serial.println(analogRead(A0));
  delay(1000);
  

}
